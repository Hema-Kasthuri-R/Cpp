#include <iostream>
using namespace std;
int main() {
    int row, col;
    int a[250][250], b[250][250], c[250][250];
    cout<<"Enter the number of rows: ";
    cin>>row;
    cout<<"Enter the number of columns: ";
    cin>>col;
    cout<<"Enter First matrix\n";
    for(int i = 0; i<row; i++)
    {
        for(int j=0 ; j<col; j++)
        {
            cout<<"Enter the element in row "<<i+1<<" and column "<<j+1<<": ";
            cin>> a[i][j];
        }
    }
   cout<<"Enter Second matrix\n";
   for(int i = 0; i<row; i++)
    {
        for(int j=0 ; j<col; j++)
        {
            cout<<"Enter the element in row " << i+1 << " and column " << j+1 <<": ";
            cin>> b[i][j];
        }
    } 
    cout<<"The Resultant matrix:\n";
   for(int i = 0; i<row; i++)
    {
        for(int j=0 ; j<col; j++)
        {
            c[i][j] = a[i][j] + b[i][j];
            cout<< c[i][j] << " ";
        }
        cout<<"\n";
    } 
    
}
