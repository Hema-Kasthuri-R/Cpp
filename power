#include <iostream>
using namespace std;
int main() {
    int base, exp, pow;
    cout<<"Enter base number: ";
    cin>>base;
    cout<<"Enter the exponent: ";
    cin>>exp;
    pow = 1;
    if(exp != 0)
    {
    for(int i = 0; i<exp ; i++)
    {
        pow = pow * base;
    }
    }
    cout<<"The result: "<<pow;
}
